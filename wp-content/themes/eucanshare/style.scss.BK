  FILE LIST
   ========================================================================== */
.page-id-78 {
	.split-content {
		.content {
			margin-bottom: 30px;
		}
	}
	.breakpage:first-child {
		margin-bottom: 0px;
	}
	.file-list {
		margin-top: 30px;
	}
}

.file-list {
	margin-bottom: 30px;
	.head-divider {
		    margin-bottom: 60px;
		    margin-top: 60px;	
	}
	.grid {
		width: 66.666%;
		float: right;

		@media only screen and (min-width: 768px) and (max-width: 959px){			
				width: 100%;
			}
			@media only screen and (max-width: 767px), 
			only screen and (min-width: 480px) and (max-width: 767px) {			
				width: 100%;
			}
		.item {
			text-align: center;
			width: calc(33.33% - 20px);
			margin: 0px;
			margin-bottom: 30px;
			margin-right: 30px;
			@media only screen and (min-width: 768px) and (max-width: 959px){			
				width: calc(33.33% - 20px);
			}
			@media only screen and (max-width: 767px), 
				only screen and (min-width: 480px) and (max-width: 767px) {			
					&.item-3 {
						width: calc(50% - 15px);				
					}
					
				&:nth-child(3n+1) {
					clear: none;
				}
				&:nth-child(2n+1) {
					clear: both;
				}
				&:nth-child(2n) {
					margin-right: 0px;
				}

			}
				
			&:nth-child(3n) {
				margin-right: 0px;
			}
		
			.item-3:nth-child(3n + 1) {
				clear: both;
			}

						
			.item-img {
				height: 120px;
				padding: 0px;
				width: 100%;
				background-image: url('css/img/lente.svg');
				margin-bottom: 30px;
				background-position: center;
			    background-size: contain;
			    background-repeat: no-repeat;	
				&.no-image {
					height: 120px;
					width: 120px;
					border-radius: 250px;
					background-color: $secondary-color;
					background-image: url('css/img/lente.svg');
					background-size: 50%;
					background-position: center;
					margin: 0 auto;
					margin-bottom: 30px;
					transition: all .2s cubic-bezier(.645,.045,.355,1);
					
				}			
			}
			.text-center {
				font-family: $font-sans;
				font-size: 13px;
				text-transform: uppercase;
				letter-spacing: 1px;
				color: $secondary-color;
				font-weight: bold;
				transition: all .2s cubic-bezier(.645,.045,.355,1);
				padding-left: 30px;
				position: relative;
				display: inline-block;
				font-size: 12px;

				&:after {
					content: '';
					width: 20px;
					height: 20px;
					background-color: rgba($primary-color, 0.5);
					position: absolute;
					left: 0px;
					top:-4px;
					border-radius: 50px;
				}
			}
			&:hover{ 
				.text-center {
					color: black;
				}
				.no-image {
					background-color: $primary-color;
				}
			}
			
		}
	}
}


