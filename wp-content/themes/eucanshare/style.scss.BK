			}
			.submit-container {
			    display: block;
			    margin-top: 20px;
			    position: relative;
			    width: 100%;
			    float: left;
			    border-top: 2px solid rgba(white,0.5);
			    padding-top: 30px;
			}
			.informativa {
				width: 70%;
				@media only screen and (max-width: 767px), 
				only screen and (min-width: 480px) and (max-width: 767px) {		
				margin: 10px 0px;
				width: 100%;	
				}

			}
			input[type="checkbox"] , .informativa {
				float: left;
			}
			 input[type="submit"]	{
				 width: auto;
				 float: right;
				 background-color: $primary-dark-color;
				 color:white;
				 &:hover {
				 background-color: white;
				 color: $primary-dark-color;					 
				 }
			 }
			
	}
		
	}
}



/* ==========================================================================
  SPLIT CONTENT
   ========================================================================== */

.split-content {
margin-bottom: 30px;
	.title {
		color: $primary-color;
		font-family: $font-sans;
		text-transform: uppercase;
		font-weight: bold;
		padding-right: 30px;	
			@media only screen and (min-width: 768px) and (max-width: 959px), 
			only screen and (max-width: 767px), 
			only screen and (min-width: 480px) and (max-width: 767px) {		
			width: 100%;
			padding-right: 0px;	
			}
		
	}
	.split-a {
		width: 33.333%;
		float: left;
		&.icon-left {
			padding-right: 60px;
		}
			@media only screen and (min-width: 768px) and (max-width: 959px), 
			only screen and (max-width: 767px), 
			only screen and (min-width: 480px) and (max-width: 767px) {		
			width: 100%;
			padding-right: 0px;	
			}		
			svg {
				width: 100%;
				height: auto;
				max-width: 100%;
			}
	}
	.split-b {
		float: left;
		width:66.666%;
		color: $secondary-color;
			@media only screen and (min-width: 768px) and (max-width: 959px), 
			only screen and (max-width: 767px), 
			only screen and (min-width: 480px) and (max-width: 767px) {		
			width: 100%;
			}			
	}
	
}


/* ==========================================================================
  TABS
   ========================================================================== */
.tab {
	.tab-header {
		width: 100%;
		margin-left: 0%;
		text-align: left;
		border-bottom: 2px solid $primary-color;
		display: block;
			@media 	only screen and (max-width: 767px), only screen and (min-width: 480px) and (max-width: 767px) {	
				width: 100%;
				margin-left: 0%;
			}

		.tab-btn {
		padding: 10px 30px 8px 30px;
	    color: $gray-color-ultralight;

	    display: inline-block;
	    cursor: pointer;
	    font-family: "Montserrat",sans-serif;
	    text-transform: uppercase;
	    font-size: 16px;
	    font-weight: bold;
	    letter-spacing: 1px;
	    position: relative;
	    -webkit-transition: color 0.2s cubic-bezier(0.645, 0.045, 0.355, 1);
	    -moz-transition: color 0.2s cubic-bezier(0.645, 0.045, 0.355, 1);
	    transition: color 0.2s cubic-bezier(0.645, 0.045, 0.355, 1);
	    border-bottom: 0px;
			@media only screen and (min-width: 768px) and (max-width: 959px){			
				    font-size: 12px;
					   
		    }
			@media 	only screen and (max-width: 767px), only screen and (min-width: 480px) and (max-width: 767px) {	
			    font-size: 12px;
			    display: inline-block;
			     padding: 10px 20px 8px 20px;
			}
			&:hover {
				background-color: rgba($primary-color , 0.2);
			}
			&.active {
				color: white;
				background-color: $primary-color;
			}
		}
	}
	.breakpage {
		background-color: $light-gray;
	}
	.tab-content {
		padding: 30px 0px;
		display: none;
		&.active {
			display: block;
		}
	}
}

.contatti{
	.tab .tab-header .tab-btn {
		@media only screen and (max-width: 767px), only screen and (min-width: 480px) and (max-width: 767px) { 
			display: block;
			text-align: center;
			border-bottom: 2px solid rgba($primary-color, 0.3);
			&:nth-child(3){
				border-bottom: 0px;
			}
		}
	}
}

/* ==========================================================================
  TEMPLATE ISOTOPE
   ========================================================================== */
.template-isotope {
	
	.item-img {
    	height: 400px;
		padding: 30px 0px;
		text-align: center;
		overflow: hidden;
			@media only screen and (max-width: 767px), 
			only screen and (min-width: 480px) and (max-width: 767px) {	
				height: 220px;
			}
		
			svg {
				width: 90%;
			}
    	}
	.content-single {
		margin: 60px auto;
	}
}


/* ==========================================================================
  PAGE DEFAULT
   ========================================================================== */
.template-default {
	
	.item-img {
    	height: 450px;
		padding: 30px 0px;
		overflow: hidden;		
		@media only screen and (max-width: 767px), 
		only screen and (min-width: 480px) and (max-width: 767px) {			
			height: 220px;
		}
    }
	.content-single {
		margin: 60px auto;
	}
}

/* ==========================================================================
   AREA ROUNDED LINK
   ========================================================================== */

.rounded-link {
	.item {
		.item-img {
			width: 200px;
			height: 200px;
			border-radius: 300px;
			margin: 0 auto;
			margin-bottom: 30px;
			@media only screen and (max-width: 767px), 
			only screen and (min-width: 480px) and (max-width: 767px) {			
				width: 100px;
				height: 100px;
				margin-bottom: 15px;
			}
		}
	font-family: $font-sans;
	text-transform: uppercase;	
	font-size: 30px;
	font-weight: 500;
	color: black;
	.content {
		font-family: $font-sans;
		text-transform: none;
		font-size: $font-size-text;
		color: $secondary-color;
		margin: 30px 0px;
		padding: 0 60px;
		line-height: $font-lh-text;
