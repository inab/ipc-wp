/* TOOLTIP */

.css-tooltip{
  position:relative;
  display:inline-block;
  .tt-content{
    position:absolute;
    
    background-color: #2d3338;
    z-index:99999999;
    box-sizing:border-box;
    padding:10px;
    min-width:180px;
        box-shadow: 0 1px 1px rgba(0, 0, 0, 0.1), 0 1px 1px rgba(0, 0, 0, 0.1);
    border-radius:1px;
    display: none;
	    p, a, strong {
		    font-size: 11px;
		    line-height: 13px;
		    color: white;
		}
    i{
      position:absolute;
      overflow:hidden;
      &:after {
        content:'';
        position:absolute;
        width:12px;
        height:12px;
        background-color: #2d3338;
      }
    }
  }
  &.right{
    .tt-content{
      top:50%;
      left:100%;
      margin-left:10px;
      transform:translate(0, -50%);
      i {
        top:50%;
        right:100%;
        margin-top:-12px;
        width:12px;
        height:24px;
        &:after {
          left:0;
          top:50%;
          transform:translate(50%,-50%) rotate(-45deg);
        }
      }
    }
  }
  &:hover{
    .tt-content{
      display:block;
    }
  }
	i.fa.fa-info-circle {
	    /* : ; */
	    padding: 2px;
	    background-color: white;
	    border-radius: 200px;
	    font-size: 16px;
	    color: #2d3338;
	}  
}









