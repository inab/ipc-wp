.altri-servizi {
	margin: 30px 0px;
	.item {
		color: $gray-color;
		padding: 20px;
		border: 1px solid $light-x2-gray;
		text-transform: uppercase;
		@media only screen and (min-width: 768px) and (max-width: 959px) {			
			width: calc(50% - 26px) ;
			float: left;
			margin-right:30px;
			&:nth-child(2n){
			margin-right:0px;
			}
			&:nth-child(2n + 1){
			clear: both;
			margin-right: 30px;
			}
			&:nth-child(3n + 1){ 
				clear: none;
			}
		}

		@media only screen and (max-width: 767px), 
		only screen and (min-width: 480px) and (max-width: 767px) {			
			width: 100%;
			float: left;
			margin-bottom: 15px;
			&:nth-child(3n + 1){ 
				clear: none;
			}
		}

		
		&.active {
		color: white;
		border: 1px solid $primary-color;		
		background-color: $primary-color;
		font-weight: bold;
		}
	}
}




.content-single {

	.date {
		margin-bottom: 30px;
		display: block;
		font-family: $font-sans;
		border-bottom: 2px solid $light-gray;
		padding-bottom: 30px;
		color: $secondary-color;
	}
	.gallery_image{
		
		.full-screen {
			position: relative;
		
				&:after {
				content: '';
				width: 100px;
				height: 100px;
				background-image: url('css/img/lente.svg');
				position: absolute;
				top:50%;
				left: 50%;
				margin-left: -50px;
				margin-top: -50px;
				opacity: 0;
				-ms-transform: scale(0);
				-webkit-transform: scale(0);
				transform: scale(0);
				-webkit-transition: all .2s cubic-bezier(.645,.045,.355,1);
				-moz-transition: all .2s cubic-bezier(.645,.045,.355,1);
				transition: all .2s cubic-bezier(.645,.045,.355,1);
			}
		}	
		&:hover {
		.full-screen:after {
			-ms-transform: scale(1);
			-webkit-transform: scale(1);
			transform: scale(1);
			opacity: 1;				
			}
		}
	}
	.wp-video {
    width: 100% !important; 
    margin: 30px 0px;
	}
	
	h1 {
		font-size: $font-size-ultra;
		line-height: $font-lh-ultra;
	}
	h2 {
		font-size: $font-size-big;
		line-height: $font-lh-big;
	}
	h3 {
		font-size: $font-size-medium;
		line-height: $font-lh-medium;
	}
	h4 {
		font-size: 18px;
		line-height: 20px;
	}
	h5 {
		font-size: 16px;
		line-height: 18px;
	}
	h6 {
		font-size: $font-size-small;
		line-height: $font-lh-small;
	}
	p {
		font-size: $font-size-text;
		line-height: 28px;
		color: $gray-color;
		margin-bottom: 20px;
	}
	img {
		margin: 30px 0px;
	}
	img.size-full {
	width: 100%;
    height: auto;
	}
	img.alignright {
    float: right;
    display: block;
    width: auto;		
	}
	img.aligncenter {
    float: none;
    display: block;
    width: auto;
    margin: 30px auto;
    max-width: 100%;
    height: auto;
    }	
}

/* ==========================================================================
  PAGINAZIONE
   ========================================================================== */

.pagination {
	font-family: $font-sans;
	margin: 30px auto;
	a[rel="next"] {
		float: right;display: inline-block;
	}
	a[rel="prev"] {
		float: left;display: inline-block;
	}
	.pagination-content {
		margin-top: 15px;		
	}
	h3 {
		color: $gray-color;
		text-transform: uppercase;
		margin-bottom: 15px;
		font-weight: normal;
	}
	h2 {
		-webkit-transition: all .2s cubic-bezier(.645,.045,.355,1);
		-moz-transition: all .2s cubic-bezier(.645,.045,.355,1);
		transition: all .2s cubic-bezier(.645,.045,.355,1);
	}
	.nav-item:hover {
		h2 {
			color: $primary-color;
		}	
	}
	.nav-prev{
		float: left;
		width: 50%;
		.pagination-content {
			float: left;
			padding: 0px 30px;
			@media only screen and (max-width: 767px), 
			only screen and (min-width: 480px) and (max-width: 767px) {			
			width: 100%;
			padding: 0px 10px;
			}			
		}
		img {
			float: left;
			border-radius: 300px;
		}
	}
	.nav-next {
		float: left;
		width: 50%;
		text-align: right;
		.pagination-content {
			float: right;
			padding: 0px 30px;
			@media only screen and (max-width: 767px), 
			only screen and (min-width: 480px) and (max-width: 767px) {			
			width: 100%;
			padding: 0px 10px;
			}
		}
		img {
			float: right;
			border-radius: 300px;
		}
	}

}

&.menu-fixed .sub-menu {
	top:85px;
}
.sub-menu {
	width: 100%;
	height: auto;
	position: fixed;
	top:109px;
	-webkit-transition: top .4s cubic-bezier(.645,.045,.355,1);
	-moz-transition: top .4s cubic-bezier(.645,.045,.355,1);
	transition: top .4s cubic-bezier(.645,.045,.355,1);
	border-bottom: 2px solid $secondary-color-light;
	z-index: 2;
@media only screen and (min-width: 768px) and (max-width: 959px) { display: none;}
@media only screen and (max-width: 767px), only screen and (min-width: 480px) and (max-width: 767px) { display: none;}
	
	li {
		width: auto;
		display: inline-block;
		float: left;
		padding: 10px 30px;
		text-align: center;	
		color: $primary-color;
		position: relative;
		background-color: white;
		cursor: pointer;
		@include shift(background-color,0.2s,cubic-bezier(.645,.045,.355,1));
		a {
		color: $primary-color;
		}
			&:after {
			content: '';
			position: absolute;
			height: calc(100% - 20px);
			top:10px;
			right:0px;
			background-color: rgba(white, 0.3);
			width: 1px;
		}
		
		&:first-child {
			padding-left: 0px;
		}
		&:hover {
			background-color:white;
			color: $primary-color;
			a {
				color: $primary-color;
			}
		}
		&.selected {
			background-color: white;
			a {
				color: $secondary-color-light;
			}
		}
		
		@media only screen and (min-width: 768px) and (max-width: 959px) {
		padding: 10px 15px;
		}
	}
	
	@media only screen and (min-width: 768px) and (max-width: 959px) {
		top: 92px;
				
	}	
	@media only screen and (max-width: 767px), only screen and (min-width: 480px) and (max-width: 767px) { 
		display: none;
		top: 0px;
		
	}	
	
}

	

	
