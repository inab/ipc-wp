@import "css/reset";
@import "css/swiper.min";
@import "css/variables";
@import "css/utilities";
@import "css/cookies";

body {
	font-family: $font-sans;
	color: $gray-color;
	
	-webkit-text-size-adjust: 100%; /* Prevent font scaling in landscape while allowing user zoom */
	
}

#main-content {
	margin-top:109px;
		@media only screen and (max-width: 767px), 
		only screen and (min-width: 480px) and (max-width: 767px) {		
			margin-top: 90px;	
	}
}




/* ==========================================================================
   COMMON 
   ========================================================================== */
sup {
    top: -0.5em;
}

sub, sup {
    font-size: 70%;
    line-height: 0;
    position: relative;
    vertical-align: baseline;
}
   

.breakpage {
	width: 100%;
	height: auto;
	display: block;
}
section.breakpage {
	margin-bottom: 60px;
		@media only screen and (max-width: 767px), 
		only screen and (min-width: 480px) and (max-width: 767px) {	
			margin-bottom: 30px;
		}	
}


.section-title {
	font-family: $font-sans;
	font-size: $font-size-big;
	color: $primary-color;
	border: 2px solid $primary-color;
	display: inline-block;
    padding: 5px 15px;
    position: relative;
    margin-left: 40px;
    	@media only screen and (max-width: 767px), only screen and (min-width: 480px) and (max-width: 767px) {
	    	font-size: $font-size-medium;
	    	margin-left: 34px;
    	}
	&:before {
		content: '';
		width: 40px;
		height: 38px;
		border: 2px solid $primary-color;
		position: absolute;
		left: -40px;
		top: -2px;
		background: repeating-linear-gradient(
	      -45deg,
	      $primary-color, /* White, starting at pixel 0 */
	      $primary-color 2px,  /* White, continuing to pixel 1 */
	      transparent 2px, /* Transparent beginning at pixel 1 */
	      transparent 8px /* Transparent ending at pixel 11 (10 + 1) */
		);
		@media only screen and (max-width: 767px), only screen and (min-width: 480px) and (max-width: 767px) {
			height: 34px;
			width: 36px;
			left: -36px;
		}
	  z-index: 0;
	}
	&:after {
		content: '';
	    width: 36px;
	    height: 16px;
	    background: white;
	    position: absolute;
	    top: 0px;
	    left: -38px;
	    z-index: 1;
	    @media only screen and (max-width: 767px), only screen and (min-width: 480px) and (max-width: 767px) {width: 32px; height: 14px; left: -34px;}
	}
	
}


.wrapper {
	width: 100%;
	position: relative;
	display: block;
	max-width: 1200px;
	margin: 0px auto;
    padding: 0 15px;
    &.fluid {
	    max-width: 100%;
    }
		@media only screen and (min-width: 768px) and (max-width: 959px) {
					padding: 0px 34px;
		}	
		@media only screen and (max-width: 767px), only screen and (min-width: 480px) and (max-width: 767px) { 
			padding: 0px 34px;
		}
}


a {
	color: $primary-color;
	&:hover {
		color: $primary-color-light;
	}
}

.blue{
	color: $primary-color;
		strong{
		color: $primary-color;
		}
}

strong{
	color: $gray-color-black;
}


strong, h1, h2, h3, h4 {
	.blue {
		color: $primary-color;
	}
}

h1 {
	font-size: $font-size-big;
	
		@media only screen and (max-width: 767px), only screen and (min-width: 480px) and (max-width: 767px) {
			font-size: $font-size-text;
		}
}

h2, h3, h4 {
	color: $gray-color;
}

p, ul, li {	
	color: $gray-color;
	font-size: $font-size-text;
	line-height: $font-lh-text;

	@media only screen and (max-width: 767px) { 
		font-size: $font-size-small;
		line-height: $font-lh-small;

	}
	@media only screen and (min-width: 480px) and (max-width: 767px) {
		font-size: $font-size-small;
		line-height: $font-lh-small;
	}	
	
	
}

.fas, .far, .fal {
	color: $primary-color;
}


.ico {
	width: 60px;
	height: 60px;
	margin: 0 auto;
	background: $primary-color;
	border-radius: 600px;
	&.big {
		width: 180px;
		height: 180px;	
	}
	&.small {
		width: 30px;
		height: 30px;
	}
	&.arrow {
		background: transparent;
		background-image: url('css/img/icon/lente.svg');
	}
}

.item-center {
	margin: 0 auto;
}


.btn{
	display: inline-block;
	background-color: white;
	color: $primary-color;	
	text-transform: uppercase;
	font-family: $font-sans;
    padding: 9px 18px 7px;
    font-size: 16px;
	font-weight: bold;
	border-radius: 1px;
	-webkit-transition: all .2s cubic-bezier(.645,.045,.355,1);
	-moz-transition: all .2s cubic-bezier(.645,.045,.355,1);
	transition: all .2s cubic-bezier(.645,.045,.355,1);
		@media only screen and (min-width: 768px) and (max-width: 959px) { 
			padding: 7px 15px;
			font-size: 14px;
		}	
	
		@media only screen and (max-width: 767px), 
		only screen and (min-width: 480px) and (max-width: 767px) {			
				padding: 7px 15px;
				font-size: 16px;
		}
	&.primary {
		background-color: $primary-color;
		color: white;	
		border: 2px solid $primary-color;
		&:hover {
			background-color: transparent;
			color: $primary-color;
		}
	}
	&.pull-right{
		float: right;	
	}
	&.btn-white {
		background-color: white;
		color: $primary-color;	
		border: 2px solid white;	
		&:hover {
		background-color: $primary-color;
		color: white;
		}			
	}
	
	&.btn-text {
		font-weight: bold;
		padding: 0px;
	}

	&.icon-right {
		padding-right: 30px;
		background: transparent;
	}

}

.title {
	font-family: $font-sans;
	color: $primary-color;
	font-size: $font-size-big;
	line-height: $font-lh-big;
}

.extra-pad {
	padding: 0px 80px;
}

/* ==========================================================================
   FORM 
   ========================================================================== */
.wpcf7, .form-helper {
	font-family: $font-sans;

	span.wpcf7-list-item {
    display: inline-block;
    margin: 0px;
    	.wpcf7-list-item-label{
	    	font-size: $font-size-text;
	    	line-height: $font-lh-text;
	    		@media only screen and (max-width: 767px), only screen and (min-width: 480px) and (max-width: 767px) { 
		    		font-size: $font-size-small;
		    		line-height: $font-lh-small;
	    		}
    	}
	}
	
	input , textarea{
		font-family: $font-sans;
	    border-radius: 0px !important;
	    -webkit-appearance: none;
	    border: 1px solid rgba($primary-color-light, 0.4);
	    border-bottom: 2px solid $primary-color-light;
	    padding: 12px 14px;
	    font-size: 15px;
	    width: auto;
	    display: block;
	    width: 100%;
	    cursor: pointer;
	}
		
	select {
		    font-size: 15px;
			border: 1px solid #d2d2d2;
			height: 30px;
			background-color: white;
	}
	.field {
		margin: 10px 0px;
		&.field-img {
			border-bottom: 1px dotted #d2d2d2;
			display: block;	
			&:last-child {
			border-bottom: 0px solid $secondary-color;
			margin-bottom: 30px;
			}
			input {
				padding-left: 0px;
				padding-top: 0px;
			}
		}
	}
	input[type="file"] {
		border: 0px;
	}
	input[type="submit"] {
		background-color: $primary-color-light;
		color: white;
		border-color: $primary-color-light;
		font-weight: bold;
		text-transform: uppercase;
		max-width: 200px;
		float: right;
			@media only screen and (max-width: 767px), only screen and (min-width: 480px) and (max-width: 767px) { 
				margin: 0 auto;
				float: none;
			}
	}
	input[disabled] {
		background: rgba($primary-color-light, 0.4);
		border-color: transparent;
	}	
	input[type="checkbox"] {
			border: 2px solid $primary-color-light;
			min-width: inherit;
			width: auto;
		    background: #FFF;
		    outline: none;
		    color: $secondary-color;
			margin: 0px;
		    -webkit-appearance: none;
		    -webkit-font-smoothing: antialiased;
		    -webkit-border-radius: 4px;
		    -moz-border-radius: 4px;
		    border-radius: 4px;
		    -webkit-transition: all 0.15s;
		    -moz-transition: all 0.15s;
		    -ms-transition: all 0.15s;
		    -o-transition: all 0.15s;
		    transition: all 0.15s;
		    margin-right: 10px;
		    border-radius: 0px;
		    float: left;
		    cursor: pointer;
		&:checked {
		    background-color: $primary-color-light;
		    border-color: $primary-color-light;
		}
		&:hover {
		    background-color: $primary-color-light;
		    border-color: $primary-color-light;		
		}
		}

	font-family: $font-sans;
	color: $gray-color;
	.field-accept {
		padding: 15px 0px;
		.label, .acceptance-538 {
			float: left;
			a {
				font-weight: bold;
				text-decoration: underline;
			}
		}
		.wpcf7-list-item {
			margin: 0px;
		}
	}

	.label {
		font-size: 12px;
	}
	.subtitle{
		font-weight: bold;
		border-bottom: 1px solid $secondary-color;
		display: inline-block;
		margin: 15px 0px;
		&:first-child {
			margin-top: 0px;
		}
		
	}
	
	
	div.wpcf7-validation-errors, div.wpcf7-acceptance-missing {
	    border: 2px solid $secondary-color-light;
	}	
	
	div.wpcf7-response-output {
		margin: 2em 0em 1em;
		font-family: $font-sans;
	}
	
	span.wpcf7-not-valid-tip {
	    color: $secondary-color-light;
	    margin-top: 5px;
	}
	
	
}


/* ==========================================================================
   UTILITIES 
   ========================================================================== */
.center {
	text-align: center;
}   
   
.cover {
	background-size: cover;
}

.img-contain {
	background-size: contain;
	background-repeat: no-repeat;
}

.cover-center {
	background-position: center center;	
}



.text-center {
	text-align: center;
}

.cover-container {
	position: absolute;
	top:0px;
	left: 0px;
	width: 100%;
	height: 100%;
	z-index: -2;
}
.vignette {
	z-index: -1;
	opacity: 1;
	
	background: $primary-color;
	
	-webkit-transition: opacity .35s cubic-bezier(.645,.045,.355,1);
	-moz-transition: opacity .35s cubic-bezier(.645,.045,.355,1);
	transition: opacity .35s cubic-bezier(.645,.045,.355,1);
}

.grain {
	z-index: -1;
	opacity: 1;
    background-image: url(css/img/grain.png);
    background-repeat: repeat;
    background-size: auto;
}

.swiper-slide .vignette {
	opacity: 0.4;
}

.swiper-slide:hover .vignette {
	opacity: 0.2;
}

.swiper-pagination-bullet {
	background: $primary-color;
}
.swiper-pagination-bullet-active {
	background: $primary-color;
}


.flex {
    -ms-box-orient: horizontal;
    display: -webkit-box;
    display: -moz-box;
    display: -ms-flexbox;
    display: -moz-flex;
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: space-around;
    justify-content: space-around;
    -webkit-flex-flow: row;
    flex-flow: row;
	.item {
	    flex: 1;
	    margin-right: 30px;
	    display: flex;
	    flex-direction: column;
	    text-align: center;
	    padding: 5px 0px;
		&:last-child {
	    	margin-right: 0px;
		}
	}
}

#video {
    width: 100%;
    height: auto;
    position: relative;
    overflow: hidden;
	
	iframe {
	    position: absolute;
	    top: 0;
	    left: 0;
	    width: 100%;
	    height: 100%;
	}
	.item-img {
	    position: relative;
	    padding-bottom: 40%;
	    height: 0;
	    overflow: hidden;
	    max-width: 100%;
	    width: 71%;
	    background: #00233c;
		margin: 0 auto;
	}

}




.item-flex {
	display: flex;
	svg {
		width: 100%;
		margin: 0 auto;
		opacity: 0;
		@media only screen and (max-width: 767px), 
		only screen and (min-width: 480px) and (max-width: 767px) {			
		transform: scale(1.5);

		}
	}
}


.divider.wrapper {
	height: 2px;
	display: block;
	width: 100%;
	margin: 60px auto;
	background-color: rgba($primary-color-light, 0.1);
	@media only screen and (max-width: 767px), 
	only screen and (min-width: 480px) and (max-width: 767px) {			
		margin: 30px auto;
	}
}
.head-divider {
	height: 2px;
	display: block;
	width: 100%;
}




/* ==========================================================================
   GRID 
   ========================================================================== */

.title {
	margin-bottom: 15px;
}

.title {
	&.underlined {
		position: relative;
		padding-top: 20px;
		&:after {
		    content: '';
		    width: 6vw;
		    height: 2px;
		    background-color: $primary-color;
		    position: absolute;
		    top: 0px;
		    left: 0px;
		    -webkit-transition: all .2s cubic-bezier(.645,.045,.355,1);
		    -moz-transition: all .2s cubic-bezier(.645,.045,.355,1);
		    transition: all .2s cubic-bezier(.645,.045,.355,1);
		}

	}
}

.title + .content {
	margin-bottom: 15px;
	font-size: $font-size-text;
	line-height: $font-lh-text;
		@media only screen and (min-width: 768px) and (max-width: 959px) { 
			line-height: $font-lh-text-meno;
		}
}


.item-3 {
	width: 33.33%;
	float: left;
	display: block;
}

.item-6 {
	width: 50%;
	float: left;
	display: block;
}

.item-2 {
	width: 50%;
	float: left;
	display: block;
			@media only screen and (max-width: 767px), 
			only screen and (min-width: 480px) and (max-width: 767px) {			
				width: 100%;
				margin-bottom: 60px;
			}	
}


.grid {
	.item {
		float: left;
	}

.item-row {
	width: 100%;
	margin-bottom: 30px;
	.item-img {
	float: left;
	width: 25%;
	height: 180px;
	}
	.img-content {
	float: left;
	width: 75%;
	padding-left: 30px;		
	}
}

	.item-2 {
		width: calc(50% - 26px) ;
		margin-right: 30px;
	}
	.item-2:nth-child(2n) {
		margin-right: 0px;
	}

	.item-2:nth-child(2n + 1) {
		clear: both;
	}

	.item-3  {
		width: calc(33.333% - 40px) ;
		margin-right: 60px;
			@media only screen and (min-width: 768px) and (max-width: 959px){
				width: calc(50% - 40px);
			}
			
			
			@media only screen and (max-width: 767px), only screen and (min-width: 480px) and (max-width: 767px) {	
			width: 100%;
			margin-right: 0px;
			padding-bottom: 30px;
			}
		
	}
	

	
	.item-3:nth-child(3n) {
		margin-right: 0px;
		@media only screen and (min-width: 768px) and (max-width: 959px), 
		only screen and (max-width: 767px) {
			margin-right: 0px;
		}
	}

	.item-3:nth-child(3n + 1) {
		clear: both;
		@media only screen and (min-width: 768px) and (max-width: 959px), 
		only screen and (max-width: 767px){
			clear: inherit;
		}
	}

	.item-3:nth-child(2n) {
		@media only screen and (min-width: 768px) and (max-width: 959px), 
		only screen and (max-width: 767px){
			margin-right: 0;
		}
	}

	.item-3:nth-child(2n + 1) {
		@media only screen and (min-width: 768px) and (max-width: 959px), 
		only screen and (max-width: 767px){
			clear: both;
			margin-right: 60px;
		}
	}


	.item-4  {
		width: calc(25% - 23px) ;
		margin-right: 30px;
		@media only screen and (min-width: 768px) and (max-width: 959px) {
			width: calc(50% - 15px) ;
			margin-right: 30px;		
			&:nth-child(2n) {
				margin-right: 0px;
			}
		} 
		@media only screen and (max-width: 767px), only screen and (min-width: 480px) and (max-width: 767px) {			
			width: calc(50% - 15px) ;
			margin-right: 30px;		
			&:nth-child(2n) {
				margin-right: 0px;
			}

		}
	}
	.item-4:nth-child(4n) {
		margin-right: 0px;
	}

	.item-4:nth-child(4n + 1) {
		clear: both;
	}


	.item-6  {
		width: calc(16.666% - 25px) ;
		margin-right: 30px;

			@media only screen and (max-width: 767px), 
			only screen and (min-width: 480px) and (max-width: 767px) {	
				width: 100%;
				margin-right: 0px;
				display: none;
				
				h3 {
					margin-bottom: 15px;
					margin-top: 15px;
				}
				
				&:first-child {
					display: block;
				}
				&:nth-child(5) {
					display: block;
				}
				&:last-child {
					display: block;
				}
			}		
	}
	.item-6:nth-child(6n) {
		margin-right: 0px;
	}

	.item-6:nth-child(6n + 1) {
		clear: both;
	}


}




/* ==========================================================================
   HEADER 
   ========================================================================== */


header {
	position: fixed;
	top: 0px;
	background: white;
	text-align: right;
	padding: 22px 0px;
    z-index: 10;	
     -webkit-backface-visibility: hidden;
    backface-visibility: hidden;    
    -webkit-transition: all .4s cubic-bezier(.645,.045,.355,1);
    -moz-transition: all .4s cubic-bezier(.645,.045,.355,1);
    transition: all .4s cubic-bezier(.645,.045,.355,1);

			@media only screen and (min-width: 768px) and (max-width: 959px) {
				padding: 15px 0px;
				
			}	
			@media only screen and (max-width: 767px), only screen and (min-width: 480px) and (max-width: 767px) { 
				padding: 15px 0px;
				
			}		
			
			
}


.top-gradient{
	position: absolute;
	top:0;
	display: block;
	height: 4px;
	width: 100%;
	@include  gradientHorizontal($primary-color-light, $secondary-color-light);
}

.menu-fixed {
	header {
		background: rgba(white,0.90);
		padding: 10px 0px;	
	}
	.main-navigation {

			@media only screen and (min-width: 768px) and (max-width: 959px), 
			only screen and (max-width: 767px), 
			only screen and (min-width: 480px) and (max-width: 767px) {		
				padding-top: 100px;	
				margin-top: 4px;
		}
	}
	.logo {
			@media only screen and (min-width: 768px) and (max-width: 959px), 
			only screen and (max-width: 767px), 
			only screen and (min-width: 480px) and (max-width: 767px) {		
				height: 30px;	
				background-image: url($main-logo-mobile);
			}
    	}
 
    
}

.site-branding {
	display: inline-block;
	float: left;
}

.logo {
    width: 200px;
    height: 65px;
	background-image: url($main-logo);
	background-repeat: no-repeat;
	background-size: 100%;
	display: block;
	z-index: 10;
	position: relative;
	background-position: left center;
			-webkit-transition: all .1s cubic-bezier(.645,.045,.355,1);
			-moz-transition: all .1s cubic-bezier(.645,.045,.355,1);
			transition: all .1s cubic-bezier(.645,.045,.355,1);
				
			@media only screen and (max-width: 767px), 
			only screen and (min-width: 480px) and (max-width: 767px) {			
			width: 150px;
			height: 62px;
			}
}




.main-navigation {
	display: inline-block;
	transform: translateY(50%);
	margin-top: 23px;
			@media only screen and (min-width: 768px) and (max-width: 959px), 
			only screen and (max-width: 767px), 
			only screen and (min-width: 480px) and (max-width: 767px) {				
				position: fixed;
				height: 100%;
				width: 100%;
				z-index: 9;
				left: 0px;
				top:0px;
				background: $light-gray;
				margin-top: 4px;
				padding: 100px 50px;
				-ms-transform: translateX(100%);
				-webkit-transform: translateX(100%);
				transform: translateZ(100%); 			
				-webkit-transition: all .2s cubic-bezier(.645,.045,.355,1);
				-moz-transition: all .2s cubic-bezier(.645,.045,.355,1);
				transition: all .2s cubic-bezier(.645,.045,.355,1);
				transition-delay: 0.1s;
		}
		
		a.menu-contatti.last {
			background-color: $secondary-color-light;
			border: 2px solid $secondary-color-light;
			color: white;
			font-weight: bold;
			border-radius: 1px;
			&:hover, &.selected{
				background: transparent;
				border: 2px solid $secondary-color-light;
				color: $secondary-color-light;
			}
			
				@media only screen and (min-width: 768px) and (max-width: 959px) { 
					display: block;
					margin: 0 auto;
					width: 150px;
				}
