				.fas {color: white;}
				&.link {padding: 0px;}
				
			/* copy link */
			.tooltip {
			  position: relative;
			  display: inline-block;
			}
			
			.tooltip .tooltiptext {
			  visibility: hidden;
			  width: auto;
			  background-color: white;
			  text-transform: uppercase;
			  font-size: 14px;
			  text-align: center;
			  border-radius: 1px;
			  padding: 2px;
			  position: absolute;
			  z-index: 1;
			  bottom: -60px;
			  left: 50%;
			  margin-left: -75px;
			  opacity: 0;
			  transition: opacity 0.3s;
			}
			
			.tooltip:hover .tooltiptext {
			  visibility: visible;
			  opacity: 1;
			}	

			button {
			    border: none;
			    background-color: transparent;
			    padding: 0px;
			    width: 36px;
			    height: 36px;
			}			
			
			
			/* copy link */
			
			
				
		}
	}
	
	.credit {
			background-color: $light-gray;
			padding: 10px 30px;
			p{
			font-size: $font-size-small;
			}
			a {
				color: $gray-color;
				&:hover{
					color: $primary-color;
				}
			}
		}

	.item-info {
		.item-date { 	
		display: inline-block;
		margin-top: 60px;
		margin-bottom: 20px;
		font-size: $font-size-small;
		text-transform: uppercase;
		font-weight: bold;
		color: $gray-color-light;
		padding-left: 15px;
		}

		.item-category {
		display: inline-block;
		font-size: $font-size-small;
		color: $gray-color-light;
		text-transform: uppercase;
		font-weight: bold;
		padding: 4px;
		border: 1px solid $gray-color-light;
		border-radius: 1px;
		}
	}
	.content-single {
		position: relative;
		display: block;
	    width: 720px;
	    margin-right: auto;
		margin-left: auto;
			@media only screen and (max-width: 767px), only screen and (min-width: 480px) and (max-width: 767px) { 
				width: 100%;
			}
			@media only screen and (min-width: 768px) and (max-width: 959px) { 
				width: 80%;
			}

		.title {
		color: $primary-color;
		}
		
		&.split-content {
		
			.title {
				color: $primary-color;
				font-family: $font-sans;
				text-transform: uppercase;
				font-weight: bold;
				float: none;

			}
			.content {
				color: $gray-color;
			}
		}
	}
	
	 .single-news {
		.title {
			
		}		 
	 }	
	
	.single-events {

		.title {
			
		}
	}
	
	
}

