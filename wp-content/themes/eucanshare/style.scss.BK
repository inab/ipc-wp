  PAGE DEFAULT
   ========================================================================== */


.page-default {
	
	.primary-image {
		height: 600px;
		width: 100%;
	}
}


/* ==========================================================================
  GALLERY
   ========================================================================== */

.gallery {
	background-color: $light-x2-gray;
	padding-top: 30px;
	overflow: hidden;
	img {
		opacity: 0;
	}
	.item {
	    height: 180px;
	    margin-bottom: 30px;
	    position: relative;
	    cursor: pointer;
		@media only screen and (max-width: 767px), only screen and (min-width: 480px) and (max-width: 767px) {			
			height: 90px;
		}
		&:after {
				content: '';
				width: 100px;
				height: 100px;
// 				background-color: $primary-color;
				background-image: url('css/img/lente.svg');
				background-size: 100px;
				position: absolute;
				top:50%;
				left: 50%;
				margin-left: -50px;
				margin-top: -50px;
				opacity: 0;
				transform: scale(0);
				transition: all .2s cubic-bezier(.645,.045,.355,1);
				border-radius: 100px;
				@media only screen and (max-width: 767px), only screen and (min-width: 480px) and (max-width: 767px) {			
					height: 90px;
					width: 50px;
					height: 50px;
					margin-left: -25px;
					margin-top: -25px;
					background-size: 50px;

				}

		}
		&:hover:after {
				opacity: 1;
				transform: scale(1);
		}
	}
}


