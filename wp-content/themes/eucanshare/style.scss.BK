/* ==========================================================================
   SOCIAL BAR ( fixed )
   ========================================================================== */
   

.social-bar{
	width: auto;
	height: 220px;
	position: fixed;
	margin-top: 30px;
	perspective: 1000px;
	z-index: 99;
	font-family: sans-serif;
	
	.social li a {
	  display: block;
	  height: 40px;
	  width: 40px;
	  background: $secondary-color-light;
	  border-bottom: 1px solid $secondary-color;
	  color: #fff;
	  -webkit-font-smoothing: antialiased;
	  padding: 12px;
	  text-decoration: none;
	  text-align: center;
	  transition: background .5s ease .300ms;
	  	@media only screen and (max-width: 767px), 
		only screen and (min-width: 480px) and (max-width: 767px){
			height: 30px;
			width: 30px;
			padding: 8px 8px 7px 6px;
		}
	  
	  	&.zenodo{
		    position: relative;
		    background-image: url('img/zenodo-logo.png');
		    background-size: 100%;
		    background-position: center center;
		    display: block;
		    span {
			        margin: -7px 46px;
		    }		    
		}
		&.rg{
		    position: relative;
		    background-image: url('img/rg-logo.png');
		    background-size: 100%;
		    background-position: center center;
		    display: block;
		    span {
			        margin: -7px 46px;
		    }
		}
	}
	
	.social li:first-child a { border-radius: 0 5px 0 0; }
	.social li:last-child a { border-radius: 0 0 5px 0; border-bottom: 0px; }
	    
	.social li a span {
	  background: $secondary-color-light;
	  display: block;
	  float: left;
	  text-align: center;
	  color: #fff;
	  margin: -22px 46px;
	  padding: 8px;
	  transform-origin: 0;
	  visibility: hidden;
	  opacity: 0;
	  border-radius: 1px;
	  -webkit-transition: all .2s cubic-bezier(.645,.045,.355,1);
	  -moz-transition: all .2s cubic-bezier(.645,.045,.355,1);
	  transition: all .2s cubic-bezier(.645,.045,.355,1);	
	  font-family: $font-sans;
	  font-size: $font-size-small;
	  font-weight: bold;
	  	@media only screen and (max-width: 767px), 
		only screen and (min-width: 480px) and (max-width: 767px) {
			display: none;
		}
	}
	
	.social li span:after {
	  content: '';
	  display: block;
	  width: 0;
	  height: 0;
	  position: absolute;
	  left: -20px;
	  top: 7px;
	}
	
	.social li a:hover span {
	  visibility: visible;
	  opacity: 1;
	  transform: rotateY(0);
	}   
}  
   




