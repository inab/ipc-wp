.accordion {
	margin-bottom: 30px;
	.accordion-head {
		border: 2px solid $primary-color;
		cursor: pointer;
		-webkit-transition: background .2s cubic-bezier(.645,.045,.355,1);
		-moz-transition: background .2s cubic-bezier(.645,.045,.355,1);
		transition: background .2s cubic-bezier(.645,.045,.355,1);
		margin-top: 30px;
		&:hover {
		background: lighten($primary-color, 0.1);
			.icon{
				background-image: url(img/plus-w.svg);
				background-size: 100%;
				background-position: center center;
			}
		}
		
		.accordion-title {
			p {
			float: left;
			width: 100%;
			margin-bottom: 0px;
			margin-top: 0px;
			padding: 8px 20px 6px 20px;
			font-size: $font-size-text;
			color: $primary-color;
			text-transform: uppercase;
			font-weight: bold;
				@media only screen and (min-width: 768px) and (max-width: 959px) { 
					padding: 5px 10px 4px 10px;
				}
				@media only screen and (max-width: 767px), only screen and (min-width: 480px) and (max-width: 767px) {
					padding: 5px 10px 4px 10px;
				}
				
				&:hover{
					color: white;	
				}
		}
		
			
		}
		span.icon {
			float: right;
			width: 20px;
			height: 20px;
			-webkit-transition: all .2s cubic-bezier(.645,.045,.355,1);
			-moz-transition: all .2s cubic-bezier(.645,.045,.355,1);
			transition: all .2s cubic-bezier(.645,.045,.355,1);
			background-image: url(img/plus.svg);
			background-size: 100%;
			background-position: center center;
			
		}	
	}

	.content {
		padding: 10px 30px 0px 10px;
		opacity: 0;
		-webkit-transition: opacity .4s cubic-bezier(.645,.045,.355,1);
		-moz-transition: opacity .4s cubic-bezier(.645,.045,.355,1);
		transition: opacity .4s cubic-bezier(.645,.045,.355,1);	
	}
	&.active {
		.accordion-title p {color: white;}
		.accordion-head {
			background: $primary-color;
			.icon {
				background-image: url(img/minus-w.svg);
				background-size: 100%;
				background-position: center center;
			}			
			
			+.accordion-content { // + tutti accordion-content che hanno come precendete accordion-head
				max-height: 1600px;
				.content {
					opacity: 1;
				}
			}
		}

	}
	.accordion-content {
		overflow: hidden;
		max-height: 0px;
		width: 100%;
		-webkit-transition: max-height .2s cubic-bezier(.645,.045,.355,1);
		-moz-transition: max-height .2s cubic-bezier(.645,.045,.355,1);
		transition: max-height .2s cubic-bezier(.645,.045,.355,1);
		&.table{
			.content{
				margin-left: 0px;
				padding-left: 0px;
			    padding-right: 0px;
			    
			    img{
				    width: 100%;
			    }
			}
		}
	}
}


