.site-footer {
  font-family: "Roboto", sans-serif;
  font-size: 14px;
  padding: 20px 0px 40px;
  color: white;
  /* fallback/image non-cover color */
  background-color: #b61f34;
  /* Firefox 3.6+ */
  background-image: -moz-linear-gradient(left, #b61f34 0%, #e4032e 100%);
  /* Safari 4+, Chrome 1+ */
  background-image: -webkit-gradient(linear, left top, right top, color-stop(0%, #b61f34), color-stop(100%, #e4032e));
  /* Safari 5.1+, Chrome 10+ */
  background-image: -webkit-linear-gradient(left, #b61f34 0%, #e4032e 100%);
  /* Opera 11.10+ */
  background-image: -o-linear-gradient(left, #b61f34 0%, #e4032e 100%);
  /* IE10+ */
  background: -ms-linear-gradient(left, #b61f34 0%, #e4032e 100%);
  /* Standard */
  background: linear-gradient(to right, #b61f34 0%, #e4032e 100%);
  /* IE6-9 */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='$from', endColorstr='$to',GradientType=1 );
}

.site-footer .copyright {
  float: left;
}

