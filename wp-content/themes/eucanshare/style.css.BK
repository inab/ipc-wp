/* TOOLTIP */
.css-tooltip {
  position: relative;
  display: inline-block;
}

.css-tooltip .tt-content {
  position: absolute;
  background-color: #2d3338;
  z-index: 99999999;
  box-sizing: border-box;
  padding: 10px;
  min-width: 180px;
  box-shadow: 0 1px 1px rgba(0, 0, 0, 0.1), 0 1px 1px rgba(0, 0, 0, 0.1);
  border-radius: 1px;
  display: none;
}

.css-tooltip .tt-content p, .css-tooltip .tt-content a, .css-tooltip .tt-content strong {
  font-size: 11px;
  line-height: 13px;
  color: white;
}

.css-tooltip .tt-content i {
  position: absolute;
  overflow: hidden;
}

.css-tooltip .tt-content i:after {
  content: '';
  position: absolute;
  width: 12px;
  height: 12px;
  background-color: #2d3338;
}

.css-tooltip.right .tt-content {
  top: 50%;
  left: 100%;
  margin-left: 10px;
  transform: translate(0, -50%);
}

.css-tooltip.right .tt-content i {
  top: 50%;
  right: 100%;
  margin-top: -12px;
  width: 12px;
  height: 24px;
}

.css-tooltip.right .tt-content i:after {
  left: 0;
  top: 50%;
  transform: translate(50%, -50%) rotate(-45deg);
}

.css-tooltip:hover .tt-content {
  display: block;
}

.css-tooltip i.fa.fa-info-circle {
  /* : ; */
  padding: 2px;
  background-color: white;
  border-radius: 200px;
  font-size: 16px;
  color: #2d3338;
